package core;
class Laptop{
	private int ram;
	private double price;
	private String model;
	public Laptop(int ram,double price,String model) {
		this.ram =ram;
		this.price=price;
		this.model=model;
	}
	@Override
	public String toString() {
		return "ram : "+ram+" price : "+price+" model : "+model;
	}
}
public class ListPractical {
	public static void main(String[] args) {
		Laptop l1 = new Laptop(8, 8999, "dell");
		Laptop l2 = new Laptop(12, 11999, "jp");
		Laptop l3 = new Laptop(16, 10999, "dell");
	}
}
